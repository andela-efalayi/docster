{
  "name": "docster",
  "version": "1.0.0",
  "description": "A fullstack document management system, complete with roles and priviledges.",
  "author": "Esther Falayi",
  "license": "ISC",
  "main": "server.js",
  "engines": {
    "node": "7.9.0",
    "npm": "4.2.0"
  },
  "scripts": {
    "build:reset:server": "NODE_ENV=production sequelize db:migrate:undo:all",
    "build:refill:server": "NODE_ENV=production sequelize db:migrate && NODE_ENV=production sequelize db:seed:all",
    "build:client": "NODE_ENV=production webpack --config webpack-production.config.babel.js && cp -R documentation build/public",
    "build:db": "npm run build:reset:server && npm run build:refill:server",
    "build:server": "babel server -d build/server --copy-files --ignore tests,devServer.js",
    "build:constants": "babel constants -d build/constants --copy-files",
    "prebuild": "rimraf build && mkdir build",
    "build": "npm run build:db && npm run build:constants && npm run build:client && npm run build:server",
    "postinstall": "npm run prebuild && npm run build",
    "reset:db": "sequelize db:migrate:undo:all",
    "seed:all": "sequelize db:migrate && sequelize db:seed:all",
    "fill:db": "npm run reset:db && npm run seed:all",
    "client:test": "jest ./client/__tests__  --coverage ",
    "mocha": "NODE_ENV=test nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 15000 --compilers js:babel-register ./server/tests/**/*.spec.js",
    "server:test": "NODE_ENV=test npm run fill:db && npm run mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start:dev": "nodemon --watch server server/devServer.js --exec babel-node",
    "start": "node build/server/server.js",
    "e2e:setup":"./node_modules/.bin/selenium-standalone install",
    "e2e:test": "node_modules/.bin/nightwatch",
    "test": "npm run server-test"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "globals": {
      "window": true
    },
    "modulePathIgnorePatterns": ["<rootDir>/client/__tests__/e2e/"],
    "setupFiles": [
      "<rootDir>/client/__mocks__/localStorageMock.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "colors": "^1.1.2",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "file-loader": "^0.11.2",
    "jest": "^20.0.4",
    "mocha": "^3.4.2",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.16",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "selenium-standalone": "^6.5.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "faker": "^4.1.0",    
    "file-loader": "^0.11.2",

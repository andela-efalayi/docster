{
  "name": "docster",
  "version": "1.0.0",
  "description": "A fullstack document management system, complete with roles and priviledges.",
  "author": "Esther Falayi",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "build:reset:server": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate:undo:all",
    "build:refill:server": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:seed:all",
    "build": "babel server -d dist",
    "reset:db": "sequelize db:migrate:undo:all",
    "seed:all": "sequelize db:migrate && sequelize db:seed:all",
    "fill:db": "npm run reset:db && npm run seed:all",
    "mocha": "NODE_ENV=test nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 15000 --compilers js:babel-register ./server/tests/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "server-test": "NODE_ENV=test npm run fill:db && npm run mocha",
    "start:dev": "nodemon --watch server server/server.js --exec babel-node",
    "start": "npm run fill:db && babel-node server/server.js",
    "test": "npm run server-test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
